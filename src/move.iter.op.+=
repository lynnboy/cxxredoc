[section#move.iter.op.+=
    [`move_iterator::operator+=]
]

[%@library operator+=[!move_iterator]]
[%@library move_iterator[!operator+=]]
[codeblock:declaration]
move_iterator& operator+=(difference_type n);
[codeblock:end]

[enter:description]
[para:effects]
[:en] [`current += n].
[:zh_CN] [`current += n]。

[para:returns]
[:en] [`*this].
[:zh_CN] [`*this]。
[exit:description]
